#vim ft:fvwm
#Set the default game location.
PipeRead 'echo SetEnv LIBRARY_HOME ${LIBRARY_HOME:=/mnt/library}'

DestroyMenu FunstuffPiMenu
AddToMenu FunstuffPiMenu
+ "========Books===========" Nop
+ "Audio Books" Menu AudioBooksMenu
+ "PDF Comix Library" exec mcomix
+ "EPUB Library" exec fbreader
+ "Dictionary" exec artha
+ "Free Book Websites" exec luakit file://$[LIBRARY_HOME]/book/usefulLinks.html
+ "========Video===========" Nop
+ "Youtube" exec smtube
+ "========Creative========" Nop
+ "Snap Programming (new scratch)" exec luakit file:///usr/local/share/applications/snap/default/snap.html
+ "Snap Manual" exec mupdf /usr/local/share/applications/snap/default/help/SnapManual.pdf
+ "Minetest" exec minetest
+ "========Games===========" Nop
+ "&C64 Games" Menu Commodore64Menu
+ "&Atari2600 Games" Menu AtariMenu
+ "&Gameboy Advance" Menu GameBoyMenu
+ "&PlayStation" Menu PlaystationMenu
