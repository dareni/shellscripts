################################### GameboyAdvance ############################
DestroyMenu GameBoyMenu
AddToMenu GameBoyMenu
+ "Beyblade G-REvolution" exec VisualBoyAdvance --no-show-speed "$[LIBRARY_HOME]/game/vba/Beyblade G-Revolution GBA.zip"
+ "Legend of Zelda" exec VisualBoyAdvance --no-show-speed -t 1 "$[LIBRARY_HOME]/game/vba/Classic NES Series - Legend of Zelda (USA, Europe).zip"
+ "Zelda II" exec VisualBoyAdvance --no-show-speed -t 1 "$[LIBRARY_HOME]/game/vba/Classic NES Series - Zelda II - The Adventure of Link (USA, Europe).zip"
+ "Finding Nemo" exec VisualBoyAdvance --no-show-speed "$[LIBRARY_HOME]/game/vba/Finding Nemo (USA, Europe).zip"
+ "Golden Sun" exec VisualBoyAdvance --no-show-speed "$[LIBRARY_HOME]/game/vba/Golden Sun (USA, Europe).zip"
+ "Golden Sun II" exec VisualBoyAdvance --no-show-speed "$[LIBRARY_HOME]/game/vba/Golden Sun - The Lost Age (USA, Europe).zip"
+ "Harry Potter" exec VisualBoyAdvance --no-show-speed "$[LIBRARY_HOME]/game/vba/Harry Potter and the Chamber of Secrets (USA, Europe) (En,Fr,De,Es,It,Nl,Pt,Sv,No,Da).zip"
+ "HarvestMoon" exec VisualBoyAdvance --no-show-speed "$[LIBRARY_HOME]/game/vba/Harvest Moon - Friends of Mineral Town (E).zip"
+ "International Karate" exec VisualBoyAdvance --no-show-speed "$[LIBRARY_HOME]/game/vba/International Karate Plus (Europe).zip"
+ "Zelda - A Link to the Past" exec VisualBoyAdvance --no-show-speed "$[LIBRARY_HOME]/game/vba/Legend of Zelda, The - A Link to the Past & Four Swords (USA, Australia).zip"
+ "Zelda - The Minish Cap" exec VisualBoyAdvance --no-show-speed "$[LIBRARY_HOME]/game/vba/Legend of Zelda, The - The Minish Cap (USA).zip"
+ "Pokemon Emerald" exec VisualBoyAdvance --no-show-speed -t 3 "$[LIBRARY_HOME]/game/vba/Pokemon - Emerald Version (USA, Europe).zip"
+ "Pokemon Fire Red" exec VisualBoyAdvance --no-show-speed -t 3 "$[LIBRARY_HOME]/game/vba/Pokemon - Fire Red Version (USA).zip"
+ "Pokemon Ruby" exec VisualBoyAdvance --no-show-speed -t 0 "$[LIBRARY_HOME]/game/vba/Pokemon - Ruby Version (USA).zip"
+ "Pokemon Saphire" exec VisualBoyAdvance -t 3 --flash-128k "$[LIBRARY_HOME]/game/vba/Pokemon - Sapphire Version (USA).zip"

